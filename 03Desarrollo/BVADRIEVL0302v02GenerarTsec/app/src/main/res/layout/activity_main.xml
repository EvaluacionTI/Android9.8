<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CV0101v01GenerarTsecWithPost">

    <TextView
        android:id="@+id/id_text_view_usuario"
        android:layout_width="404dp"
        android:layout_height="41dp"
        android:layout_marginTop="40dp"
        android:text="@string/et_usuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/id_edit_text_usuario"
        android:layout_width="392dp"
        android:layout_height="44dp"
        android:layout_marginTop="16dp"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_text_view_usuario" />

    <TextView
        android:id="@+id/id_text_view_password"
        android:layout_width="388dp"
        android:layout_height="40dp"
        android:layout_marginTop="12dp"
        android:text="@string/et_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_edit_text_usuario" />

    <EditText
        android:id="@+id/id_edit_text_password"
        android:layout_width="382dp"
        android:layout_height="46dp"
        android:layout_marginTop="12dp"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_text_view_password" />

    <Button
        android:id="@+id/id_button_generar_tsec"
        android:layout_width="393dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:text="@string/btn_generar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_edit_text_password" />

    <TextView
        android:id="@+id/id_text_view_tsec"
        android:layout_width="392dp"
        android:layout_height="142dp"
        android:layout_marginTop="32dp"
        android:text="@string/et_tsec"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_button_generar_tsec"
        app:layout_constraintVertical_bias="0.035" />

</androidx.constraintlayout.widget.ConstraintLayout>