<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CV05ConsultaById">

    <EditText
        android:id="@+id/id_et_numero_cuenta_origen"
        android:layout_width="159dp"
        android:layout_height="31dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/id_tv_moneda_cuenta_origen"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_numero_cuenta_origen" />


    <EditText
        android:id="@+id/id_et_moneda_cuenta_origen"
        android:layout_width="167dp"
        android:layout_height="22dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/id_tv_importe_transferir"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_moneda_cuenta_origen" />

    <EditText
        android:id="@+id/id_et_importe_transferir"
        android:layout_width="213dp"
        android:layout_height="23dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/id_tv_nombre_beneficiario"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_importe_transferir" />

    <EditText
        android:id="@+id/id_et_nombre_beneficiario"
        android:layout_width="213dp"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/id_tv_numero_cuenta_beneficiario"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_nombre_beneficiario" />


    <EditText
        android:id="@+id/id_et_numero_cuenta_beneficiario"
        android:layout_width="206dp"
        android:layout_height="17dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/id_tv_referencia_ordenante"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_numero_cuenta_beneficiario" />

    <EditText
        android:id="@+id/id_et_referencia_ordenante"
        android:layout_width="204dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_referencia_ordenante"
        tools:layout_editor_absoluteX="103dp" />

    <EditText
        android:id="@+id/id_et_amount_comision"
        android:layout_width="213dp"
        android:layout_height="24dp"

        android:layout_marginTop="12dp"
        android:layout_marginEnd="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_et_referencia_ordenante" />

    <EditText
        android:id="@+id/id_et_type_rates"
        android:layout_width="206dp"
        android:layout_height="17dp"

        android:layout_marginTop="16dp"
        android:layout_marginEnd="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_et_amount_comision" />

    <EditText
        android:id="@+id/id_et_amount_equivalent"
        android:layout_width="389dp"
        android:layout_height="27dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_amount_equivalent" />

    <EditText
        android:id="@+id/id_et_referencia_transferencia"
        android:layout_width="396dp"
        android:layout_height="20dp"

        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_referencia_transferencia"
        tools:layout_editor_absoluteX="6dp" />

    <EditText
        android:id="@+id/id_et_state_transferencia"
        android:layout_width="153dp"
        android:layout_height="26dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/id_et_amount_equivalent"
        tools:layout_editor_absoluteX="117dp" />

    <TextView
        android:id="@+id/id_tv_numero_cuenta_origen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/tv_numero_cuenta_origen"
        app:layout_constraintStart_toStartOf="@+id/id_et_numero_cuenta_origen"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/id_tv_moneda_cuenta_origen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/tv_moneda_cuenta_origen"
        app:layout_constraintStart_toStartOf="@+id/id_et_moneda_cuenta_origen"
        app:layout_constraintTop_toBottomOf="@+id/id_et_numero_cuenta_origen" />

    <TextView
        android:id="@+id/id_tv_importe_transferir"
        android:layout_width="176dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_importe_transferir"
        app:layout_constraintStart_toStartOf="@+id/id_et_importe_transferir"
        app:layout_constraintTop_toBottomOf="@+id/id_et_moneda_cuenta_origen" />


    <TextView
        android:id="@+id/id_tv_nombre_beneficiario"
        android:layout_width="115dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_nombre_beneficiario"
        app:layout_constraintStart_toStartOf="@+id/id_et_nombre_beneficiario"
        app:layout_constraintTop_toBottomOf="@+id/id_et_importe_transferir" />

    <TextView
        android:id="@+id/id_tv_numero_cuenta_beneficiario"
        android:layout_width="209dp"
        android:layout_height="29dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_numero_cuenta_beneficiario"
        app:layout_constraintStart_toStartOf="@+id/id_et_numero_cuenta_beneficiario"
        app:layout_constraintTop_toBottomOf="@+id/id_et_nombre_beneficiario" />

    <TextView
        android:id="@+id/id_tv_referencia_ordenante"
        android:layout_width="115dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_referencia_origen"
        app:layout_constraintStart_toStartOf="@+id/id_et_referencia_ordenante"
        app:layout_constraintTop_toBottomOf="@+id/id_et_numero_cuenta_beneficiario" />

    <TextView
        android:id="@+id/id_tv_amount_comision"
        android:layout_width="115dp"
        android:layout_height="27dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_amount_comision"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toStartOf="@+id/id_et_amount_comision"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_et_referencia_ordenante" />

    <TextView
        android:id="@+id/id_tv_type_rates"
        android:layout_width="61dp"
        android:layout_height="24dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:text="@string/tv_tipo_cambio"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toStartOf="@+id/id_et_type_rates"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_amount_comision" />

    <TextView
        android:id="@+id/id_tv_amount_equivalent"
        android:layout_width="142dp"
        android:layout_height="29dp"
        android:layout_marginTop="20dp"
        android:text="@string/tv_amount_equivalent"
        android:textColor="@color/colorPrimary"
        app:layout_constraintTop_toBottomOf="@+id/id_et_type_rates"
        tools:layout_editor_absoluteX="25dp" />


    <TextView
        android:id="@+id/id_tv_referencia_transferencia"
        android:layout_width="156dp"
        android:layout_height="22dp"
        android:layout_marginTop="12dp"
        android:text="@string/tv_referencia_pago"
        android:textColor="@color/colorAccent"
        app:layout_constraintTop_toBottomOf="@+id/id_tv_estado_transferencia"
        tools:layout_editor_absoluteX="6dp" />

    <TextView
        android:id="@+id/id_tv_estado_transferencia"
        android:layout_width="61dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:text="@string/tv_state_pago"
        android:textColor="@color/colorAccent"
        app:layout_constraintEnd_toStartOf="@+id/id_et_state_transferencia"
        app:layout_constraintTop_toBottomOf="@+id/id_et_amount_equivalent" />

</androidx.constraintlayout.widget.ConstraintLayout>